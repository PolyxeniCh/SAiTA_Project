<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.SAiTAProjectGroup.saita_project">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="SearchActivity"></activity>
        <activity android:name="CalendarActivity"></activity>
        <activity android:name="PassengersActivity"></activity>
        <activity android:name="TravelClassActivity"></activity>
        <activity android:name="FlightsActivity"></activity>
        <activity android:name="DetailsActivity"></activity>
        <activity android:name="SettingsActivity"></activity>

    </application>

</manifest>